{
  "name": "kys",
  "version": "0.0.0-alpha0",
  "description": "TWSE Securities Identification Number (SIN) Parser",
  "type": "module",
  "main": "src/main.js",
  "files": [
    "src",
    "!**/*.test.js",
    "types/main.d.ts"
  ],
  "exports": {
    ".": {
      "require": "./src/main.js",
      "default": "./src/main.js",
      "types": "./types/main.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "lint": "eslint ./src",
    "test": "tap run --allow-empty-coverage --disable-coverage --timeout=60000",
    "test:coverage": "tap run --show-full-coverage --timeout=60000 --coverage-report=text --coverage-report=lcov",
    "build:types": "tsc --noCheck",
    "pack": "npm run build:types && npm pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LsYueh/know-your-sin.git"
  },
  "keywords": [
    "TWSE",
    "Securities"
  ],
  "author": "LsYueh",
  "license": "BSD-2-Clause",
  "devDependencies": {
    "@types/node": "^24.2.1",
    "eslint": "^9.33.0",
    "globals": "^16.3.0",
    "semver": "^7.7.2",
    "tap": "^21.1.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18"
  }
}
